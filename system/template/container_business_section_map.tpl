<div class="wrapper listing_detail_view_section_wrapper listing_detail_view_map">
    <div class="wrapper listing_detail_view_section_title_wrapper expand_trigger">
        <div class="container listing_detail_view_section_title_container"><h3>Map</h3></div>
    </div>
    <div class="wrapper listing_detail_view_section_content_wrapper expand_wrapper">
        <div data-place-id="[[*place_id]]" class="container listing_detail_view_section_content_container expand_container listing_detail_view_map_frame_container"></div>
    </div>
    <div itemprop="geo" itemscope="" itemtype="http://schema.org/GeoCoordinates"><meta itemprop="latitude" content="[[*latitude]]"><meta itemprop="longitude" content="[[*longitude]]"></div>
</div>